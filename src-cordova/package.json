{
  "name": "org.mobistudy.app",
  "displayName": "Mobistudy",
  "version": "0.4.6",
  "description": "The participants' app of Mobistudy",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Mobistudy team",
  "license": "MIT",
  "devDependencies": {
    "cordova-android": "^14.0.1",
    "cordova-ios": "^7.1.1",
    "cordova-plugin-audioinput": "github:edimuj/cordova-plugin-audioinput",
    "cordova-plugin-ble-central": "^2.0.0-slim",
    "cordova-plugin-device": "^3.0.0",
    "cordova-plugin-file": "^8.1.3",
    "cordova-plugin-geolocation": "^5.0.0",
    "cordova-plugin-insomnia": "^4.3.0",
    "cordova-plugin-local-notification": "^1.2.1",
    "cordova-plugin-pedometer": "github:dariosalvi78/cordova-plugin-pedometer",
    "cordova-plugin-pincheck": "0.0.6",
    "cordova-plugin-secure-storage-echo": "github:mibrito707/cordova-plugin-secure-storage-echo",
    "cordova-plugin-splashscreen": "^6.0.2",
    "cordova-plugin-vibration": "^3.1.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-insomnia": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-pedometer": {},
      "cordova-plugin-pincheck": {},
      "cordova-plugin-secure-storage-echo": {},
      "cordova-plugin-device": {},
      "cordova-plugin-file": {
        "ANDROIDX_WEBKIT_VERSION": "1.4.0"
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-audioinput": {},
      "cordova-plugin-ble-central": {
        "BLUETOOTH_USAGE_DESCRIPTION": "This app needs to connect to Bluetooth devices",
        "IOS_INIT_ON_LOAD": "false",
        "BLUETOOTH_RESTORE_STATE": "false",
        "ACCESS_BACKGROUND_LOCATION": "false"
      },
      "cordova-plugin-local-notification": {
        "ANDROIDX_CORE_VERSION": "1.12.0"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "dependencies": {
    "cordova": "^13.0.0"
  }
}